package modules;

import java.awt.BorderLayout;

import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTabbedPane;

import implementations.CustomButton;

public class ManualInput {
	JTabbedPane parent;
	JPanel content;
	

	
	public ManualInput(JTabbedPane parent) {
		this.parent = parent;
		initPanel();
	}


	public void initPanel() {
		content = new JPanel();
		content.setLayout(new BorderLayout());
		content.add(new CustomButton("Almafa gombja"), BorderLayout.PAGE_END);
		content.add(new CustomButton("Almafa gombja2"), BorderLayout.PAGE_END);
		parent.getName();
		parent.addTab("Almafa", content);
	}
	
	
	
}
